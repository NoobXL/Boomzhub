local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/RegularVynixu/IreXion-UI-Library/main/IreXion%20UI%20Library"))()
 
local Gui = Library:AddGui({
        Title = {"Throw People", "BoomZ Hub"},
        ThemeColor = Color3.fromRGB(300, 0, 0),
        ToggleKey = Enum.KeyCode.RightControl,
})
 
local Tab = Gui:AddTab("Main")
 
local Category = Tab:AddCategory("Throw People Simulator lol")

-----
local Toggle = Category:AddToggle("AutoHold", false, function(e)
AutoHold = e
end)


spawn(function()
while wait() do
if AutoHold then
pcall(function()
while true do
    delay(0,function()
        for _,player in pairs(game.Players:GetPlayers()) do
            if player ~= game.Players.LocalPlayer then
                game:GetService("ReplicatedStorage").Remotes.Interaction:FireServer(player.Character.HumanoidRootPart)
            end
        end
    end)
 wait()
 num = num + 1
 print("Running : ", num)
end
end)
end
end
end)
---

local Toggle = Category:AddToggle("AutoThrow", false, function(a)
AutoThrow = a
end)

spawn(function()
while wait() do
if AutoThrow then
pcall(function()
local args = {
    [1] = Vector3.new(0, 3, 0),
    [2] = 9999999999999
}

game:GetService("ReplicatedStorage").Remotes.Throw:FireServer(unpack(args))
end)
end
end
end)
----------
local Tab = Gui:AddTab("Teleport")
local Category = Tab:AddCategory("Teleport")
local Label = Category:AddLabel("Teleport Players")

players = {}
 
for i,v in pairs(game:GetService("Players"):GetChildren()) do
   table.insert(players,v.Name)
end

local Box = Category:AddBox("Select Player", function(abc)
      Select = abc
end)

local Button = Category:AddButton("Teleport", function()
 game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players[Select].Character.HumanoidRootPart.CFrame
end)
----------
local Tab = Gui:AddTab("Settings")
local Category = Tab:AddCategory("Settings")
local Label = Category:AddLabel("Setting Players")

local Box = Category:AddBox("WalkSpeed", function(w)
game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = w
end)

local Box = Category:AddBox("JumpPower", function(j)
game.Players.LocalPlayer.Character.Humanoid.JumpPower = j
end)

local Button = Category:AddButton("Rejoin", function()
 loadstring(game:HttpGet("https://raw.githubusercontent.com/Exunys/Rejoin-Game/main/Rejoin%20Game.lua"))()
end)

local Bind = Category:AddBind("Toggle Hide Ui", Gui.ToggleKey, function()
        print("Toggled GUI")
end)
